#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <math.h>
int main(){
	setlocale(LC_ALL, "");
	int qtd;
	
	printf("Digite a quantidade de nï¿½meros: ");
	scanf("%i",&qtd);
	//Soma normal
	int i, soma = 0;
	for(i = 1; i <= qtd; i++){
		soma += i;
	}
	printf("SOMA: %i\n",soma);
	//Soma dos quadrados
	int somaQ = 0;
	for(i = 1; i <= qtd; i++){
		somaQ += pow(i,2);
	}
	printf("SOMA DOS QUADRADOS: %i\n",somaQ);
	//Soma dos cubos
	int somaC = 0;
	for(i = 1; i <= qtd; i++){
		somaC += pow(i,3);
	}
	printf("SOMA DOS CUBOS: %i\n",somaC);
	
	return 0;
}
